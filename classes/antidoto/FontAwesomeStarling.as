		private var _fontSize:Number;
		private var _fontColor:uint;
		
		public function FontAwesomeStarling(fontSize:Number, fontColor:uint) {
			this._fontSize = fontSize;
			this._fontColor = fontColor;
		}
		
		public function getIconTextField(char:String):TextField {
			//var textField:TextField = new TextField(30, 30, char, 'FontAwesome', this._fontSize, this._fontColor);
			var textField:TextField = new TextField(30, 30, char);
			textField.format.size = this._fontSize;
			textField.format.color = this._fontColor;
			textField.format.font = 'FontAwesome';
			textField.autoSize = TextFieldAutoSize.BOTH_DIRECTIONS;
			return textField;
		}
		
		public function getIconImage(char:String):Image {
			var textField:TextField = this.getIconTextField(char);
			textField.dispose();
			var texture:RenderTexture = new RenderTexture(textField.width, textField.height);
			
			texture.draw(textField);
			var image:Image = new Image(texture);
			return image;
		}
		
		public function set fontSize(value:Number):void {
			this._fontSize = value;
		}
		
		public function set fontColor(value:uint):void {
			this._fontColor = value;
		}
